{
  "npmScope": "applications",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "annotation": {
      "tags": ["pkg"]
    },
    "check-type": {
      "tags": ["pkg"]
    },
    "date-math": {
      "tags": ["pkg"]
    },
    "dev-app": {
      "tags": []
    },
    "fedaco": {
      "tags": ["pkg"]
    },
    "triangle-api": {
      "tags": []
    },
    "vector-math": {
      "tags": ["pkg"]
    }
  }
}
