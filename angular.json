{
  "version": 1,
  "projects": {
    "annotation": {
      "projectType": "library",
      "root": "src/annotation",
      "sourceRoot": "src/annotation/src",
      "prefix": "applications",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "src/annotation/src/**/*.ts",
              "src/annotation/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/src/annotation"],
          "options": {
            "jestConfig": "src/annotation/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "check-type": {
      "projectType": "library",
      "root": "src/check-type",
      "sourceRoot": "src/check-type/src",
      "prefix": "applications",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "src/check-type/src/**/*.ts",
              "src/check-type/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/src/check-type"],
          "options": {
            "jestConfig": "src/check-type/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "date-math": {
      "projectType": "library",
      "root": "src/date-math",
      "sourceRoot": "src/date-math/src",
      "prefix": "applications",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "src/date-math/src/**/*.ts",
              "src/date-math/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/src/date-math"],
          "options": {
            "jestConfig": "src/date-math/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "dev-app": {
      "projectType": "library",
      "root": "src/dev-app",
      "sourceRoot": "src/dev-app",
      "prefix": "",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["src/dev-app/tsconfig.json"],
            "exclude": ["**/node_modules/**", "!src/dev-app/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/src/dev-app"],
          "options": {
            "jestConfig": "src/dev-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "fedaco": {
      "projectType": "library",
      "root": "src/fedaco",
      "sourceRoot": "src/fedaco/src",
      "prefix": "applications",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "src/fedaco/src/**/*.ts",
              "src/fedaco/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/src/fedaco"],
          "options": {
            "jestConfig": "src/fedaco/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "triangle-api": {
      "root": "apps/triangle-api",
      "sourceRoot": "apps/triangle-api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/triangle-api",
            "main": "apps/triangle-api/src/main.ts",
            "tsConfig": "apps/triangle-api/tsconfig.app.json",
            "assets": [
              "apps/triangle-api/src/assets",
              "apps/triangle-api/src/.env.sample",
              "apps/triangle-api/src/package.json"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/triangle-api/src/environments/environment.ts",
                  "with": "apps/triangle-api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "triangle-api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/triangle-api/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/triangle-api"],
          "options": {
            "jestConfig": "apps/triangle-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "vector-math": {
      "projectType": "library",
      "root": "src/vector-math",
      "sourceRoot": "src/vector-math/src",
      "prefix": "applications",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "src/vector-math/src/**/*.ts",
              "src/vector-math/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/src/vector-math"],
          "options": {
            "jestConfig": "src/vector-math/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": false
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "diagram-demo"
}
