{
  "extends": "../bazel-tsconfig-build.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "../../dist/packages/material",
    "rootDir": ".",
    "rootDirs": [
      ".",
      "../../dist/packages/material"
    ],
    "importHelpers": false,
    "module": "commonjs",
    "target": "es5",
    "types": [
      "jasmine"
    ],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "paths": {
      "@angular/color": [
        "../../dist/packages/color"
      ],
      "@angular/cdk/*": [
        "../../dist/packages/cdk/*"
      ]
    }
  },
  "include": [
    "**/index.ts",
    "**/*.spec.ts"
  ],
  "exclude": [
    "**/schematics/**/*.ts",
    "**/*.e2e.spec.ts"
  ]
}
