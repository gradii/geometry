{
  "compilerOptions": {
    "experimentalDecorators": true,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "lib": ["es2015", "dom", "es2016.array.include"],
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "../../dist/tools/gulp",
    "strictNullChecks": false,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "noEmitOnError": true,
    "noImplicitAny": false,
    "downlevelIteration": true,
    "target": "es5",
    "types": [
      "node"
    ],
    "baseUrl": ".",
    "paths": {
      "@gradii/annotation": [
        "libs/annotation/public-api.ts",
        "../../libs/annotation/public-api.ts"
      ],
      "@gradii/check-type": [
        "libs/check-type/public-api.ts",
        "../../libs/check-type/public-api.ts"
      ],
      "@gradii/fedaco": [
        "libs/fedaco/public-api.ts",
        "../../libs/fedaco/public-api.ts"
      ]
    }
  },
  "files": [
    "gulpfile.ts"
  ]
}
